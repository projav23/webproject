
        <div class="card">
          {{#ifeq esport "Tenis"}}
            {{#ifeq acceptedGuests.length numberPlayers}}
            <img src="/images/tenis-logo-rojo.jpg" alt="pelota" style="width:215px;height:205px">
            {{else}}
            <img src="/images/tenis-logo.jpg" alt="pelota" style="width:215px;height:205px">
            {{/ifeq}}
          {{else}}
            <img src="/images/tenis-logo.jpg" alt="pelota" style="width:215px;height:205px">
          {{/ifeq}}
          <h1>{{esport}}</h1>
          <p class="title">Localidad: {{location}}</p>
          <p class="title">Nivel: {{level}}</p>
          <p class="center">{{center}}</p>
          <div class="row">
            <p class="date">{{dateFormat date "DD/MM/YYYY"}}</p>
            <p class="players"> Players: {{acceptedGuests.length}}/{{numberPlayers}}</p>
          </div>
          {{#ifeq acceptedGuests.length numberPlayers}}
          <p><a href="/matches/{{_id}}" style="background-color: red;">See more </a></p>
          {{else}}
          <p><a href="/matches/{{_id}}">See more </a></p>
          {{/ifeq}}
        </div>
