
<section>
  {{> menu}}
</section>

      <section id="one">
      <div id="header">
        <h1 id="homePageTitle" class="padding-left shadow-title-h1">Reta y Gana a tus amigos</h1>
        <div class="flex-row top-space">
              <p class="text-weight-h1 shadow-title-h1">Empieza hoy</p>
              <a class="btnRedondo" href="/matches/newgame">-></a>  
        </div>
        
      </div>
      
      </section>
      <section id="two" class="holster">
    <h2 class="shadow-title-h2">Ranking</h2>
    <div id="ranking" class="container x mandatory-scroll-snapping">
      <div class="text-center">
        <h3 class="shadow-title-h3">Mejores Jugadores Tennis</h3>
      <table id="table" class="container x mandatory-scroll-snapping">
        <td>
          <table class="width-100vw">
                <thead class="sendToBack-1">
                      <td id="td-title">INDIVIDUAL</td>
                </thead>
                <tbody>
                    {{#each users}}  
                    {{>bestPlayers}}
                    {{/each}}
                </tbody>
          </table>
        </td>
        <td>
          <table class="width-100vw">
                <thead class="sendToBack-1">
                  <td id="td-title">DOBLES</td>
            </thead>
            <tbody>
                {{#each users}}  
                {{>bestPlayers}}
                {{/each}}
            </tbody>
        </table>
        </td>
      </table>
    </div>
    <div class="text-center">
        <h3 class="shadow-title-h3">Mejores Jugadores Paddle</h3>
      <table id="table">
      {{#each users}}  
      {{>bestPlayers}}
      {{/each}}
      </table>
    </div>
    </div>
      </section>
      <section id="three">
        <h3 id="h3-video" class="shadow-title-h1 sendToBack">Empieza hoy a ganar partidos</h3>
        <a  id="btn-video" class="btnRedondo sendToBack" href="/matches/newgame">-></a>
      <video id="video" class="sendToBack" loop autoplay muted>
        <source src="../videos/mobile.mp4" type="video/mp4">
      </video>
      </section>


<footer>
  {{> footer}}
</footer>



