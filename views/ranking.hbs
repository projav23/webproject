<div id="tab-ranking" class="tab">
  <button class="tablinks sendToBack btn-ranking" onclick="openTabRanking(event, 'Tenis')" id="defaultOpenRanking">Tenis</button>
  <button id="btn-ranking" class="tablinks sendToBack " onclick="openTabRanking(event, 'Paddle')">Paddle</button>
</div>

